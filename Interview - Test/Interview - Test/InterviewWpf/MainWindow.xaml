<Window x:Class="InterviewWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Interview;assembly=InterviewLibrary"
        Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <local:MyDocumentProvider x:Key="documents"/>
    </Window.Resources>
    <Grid>
        <ListBox Width="400" Margin="10" x:Name="TestName" ItemsSource="{Binding Source={StaticResource documents}}">
            
        </ListBox>
    </Grid>
</Window>
